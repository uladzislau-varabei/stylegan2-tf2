{
    "model_name": "lsun_living_room",
    "images_paths_filename": "local_living_room_100k.txt",
    "target_resolution": 256,
    "latent_size": 512,
    "dlatent_size": 512,
    "resample_kernel": [
        1,
        3,
        3,
        1
    ],
    "data_format": "NCHW",
    "use_GPU_for_Gs": true,
    "use_mixed_precision": true,
    "num_fp16_resolutions": 5,
    "conv_clamp": 256,
    "fused_bias_act": true,
    "use_XLA": true,
    "implementation": "custom_grad",
    "total_kimages": 20000,
    "batch_size": 4,
    "G_loss_fn": "G_logistic_ns_pathreg",
    "D_loss_fn": "D_logistic_simplegp",
    "G_loss_fn_params": {
        "pl_batch_shrink": 2,
        "pl_decay": 0.01,
        "pl_weight": 2.0
    },
    "D_loss_fn_params": {
        "r1_gamma": 10.0
    },
    "G_learning_rate": 0.001,
    "D_learning_rate": 0.001,
    "max_models_to_keep": 5,
    "summary_scalars_every_kimages": 2.5,
    "summary_hists_every_kimages": 25,
    "save_model_every_kimages": 100,
    "save_images_every_kimages": 2.5,
    "run_metrics_every_kimages": 50,
    "metrics": {
        "FID": {
            "num_samples": 20000
        },
        "PPL": {
            "num_samples": 20000,
            "space": "w",
            "sampling": "full",
            "crop_face": false
        }
    },
    "normalize_latents": true,
    "mapping_layers": 8,
    "mapping_units": 512,
    "mapping_lrmul": 0.01,
    "mapping_activation": "leaky_relu",
    "G_architecture": "skip",
    "fused_modconv": true,
    "use_noise": true,
    "randomize_noise": true,
    "G_activation": "leaky_relu",
    "G_kernel_size": 3,
    "G_fmap_base": 8192,
    "G_fmap_max": 512,
    "use_G_smoothing": true,
    "truncation_psi": 0.7,
    "truncation_cutoff": 8,
    "dlatent_avg_beta": 0.995,
    "style_mixing prob": 0.9,
    "D_architecture": "resnet",
    "D_activation": "leaky_relu",
    "D_kernel_size": 3,
    "D_fmap_base": 8192,
    "D_fmap_max": 512,
    "mbstd_group_size": 2,
    "mbstd_num_features": 1,
    "dataset_n_parallel_calls": 6,
    "dataset_n_prefetched_batches": 4,
    "dataset_max_kimages": 100,
    "shuffle_dataset": true,
    "mirror_augment": false
}